{
  "name": "t-scanning",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "t-scanning/src",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project t-scanning --web",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/t-scanning"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "t-scanning:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "t-scanning:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "t-scanning:build:production",
          "hmr": false
        }
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "t-scanning:build"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "reportsDirectory": "../../coverage/t-scanning"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["t-scanning/**/*.{ts,tsx,js,jsx}"]
      }
    }
  }
}
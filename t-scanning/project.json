{
  "name": "t-scanning",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "t-scanning/src",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project t-scanning --web",
  "targets": {
    "build": {
      "executor": "@nrwl/web:webpack",
      "options": {
        "outputPath": "dist/apps/t-scanning",
        "index": "t-scanning/src/index.html",
        "main": "t-scanning/src/main.tsx",
        "polyfills": "t-scanning/src/polyfills.ts",
        "tsConfig": "t-scanning/tsconfig.app.json",
        "assets": ["t-scanning/src/assets"],
        "styles": ["t-scanning/src/styles.css"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "optimization": true,
          "sourceMap": false,
          "extractCss": true
        }
      }
    }
  }
}
